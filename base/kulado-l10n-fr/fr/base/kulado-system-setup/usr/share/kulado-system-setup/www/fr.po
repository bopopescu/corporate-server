#: usr/share/univention-system-setup/www/ApplianceWizard.js:374
msgid "Default system locale"
msgstr "Paramètres locaux par défaut"

#: usr/share/univention-system-setup/www/ApplianceWizard.js:604
msgid "Do not use any domain"
msgstr "Ne pas utiliser de domaine"

#: usr/share/univention-system-setup/www/ApplianceWizard.js:1752
msgid "Domain and host configuration"
msgstr "Configuration de l'hôte et du domaine"

#: usr/share/univention-system-setup/www/ApplianceWizard.js:411
msgid "Domain and network configuration"
msgstr "Configuration du réseau et du domaine"

#: usr/share/univention-system-setup/www/ApplianceWizard.js:625
msgid "Domain controller backup"
msgstr "Contrôleur de domaine de secours"

#: usr/share/univention-system-setup/www/ApplianceWizard.js:637
msgid "Domain controller slave"
msgstr "Contrôleur de domaine esclave"

#: usr/share/univention-system-setup/www/ApplianceWizard.js:715
